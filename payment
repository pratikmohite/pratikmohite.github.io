<script
	type="text/javascript"
	src="/js/jquery-1.7.2.min.js"></script>
<script
	type="text/javascript"
	src="/js/jquery-ui.min.js"></script>
<script
	type="text/javascript" src="/js/validate.js"></script>
<script
	type="text/javascript"
	src="/js/jquery.numeric.js"></script>

<script type="text/javascript">
$(".numeric").numeric({ decimal : "." });

$(document).ready(function() {
 	
	$("#pname").text("");
	$("#padd").text("");
	$("#pdet").text("");
	$("#annval").text("");
	$("#your-payable").text("");
	$("#reqstatus").text("");
	$("#proprty_name").val("");
	$("#amt").val("");
	$("#paybtn").html("");
	$(".property_owner").hide();
	$(".annual-retable").hide();
	$(".property_details_inner").hide();
	$("#paytble").hide();
	$(".property_payment").hide();
	$(".cval").val("");

	$("#search").click(function(){
		$("#pname").text("");
		$("#padd").text("");
		$("#pdet").text("");
		$("#annval").text("");
		$("#your-payable").text("");
		$("#reqstatus").text("");
		$("#proprty_name").val("");
		$("#amt").val("");
		$("#paybtn").html("");
		$(".property_owner").hide();
		$(".annual-retable").hide();
		$(".property_details_inner").hide();
		$("#paytble").hide();
		$(".property_payment").hide();
		$(".cval").val("");
		$("#popDiv").remove();
		
		if($("#proprty_no").val() != ""){
			var pno = $("#proprty_no").val();
			var total = 0;

			$('div#last_msg_loader').html("<img src='/images/loader.gif' width='45' height='25'>");

				$.post("/bill/propertysearch/"+$("#proprty_no").val(), function(res){
			
				if(res.name != ""){
					$("#pname").text(res.name);
					$("#padd").text(res.address);
					$("#pdet").text(res.details);
					$("#annval").text(" Rs. "+res.annual);
					$("#your-payable").text(" Rs. "+res.total);
					$("#proprty_name").val(res.name);
					$("#amt").val(res.total);
					$(".property_owner").show();
					$(".annual-retable").show();
					$(".property_details_inner").show();
					$('div#last_msg_loader').empty();
					$("#paybtn").show();
					if(res.total>0){
						$("#paybtn").html('<input type="button" id="makepay" value="Make Payment Online" class="blueBtn">');
						$("#paytble").show();
					}
				}
				else{
					$('div#last_msg_loader').empty();
					alert("No record available");
				}
			},"json");		
		}
		else{
			alert("Please enter the property number");			
		}
	});

	$("#paybtn").on("click","#makepay",function(){

			$("body").append('<div id="background"></div><div id="large"><p><span>Disclaimer:-</span> The following rates will be applicable over and above '+
					'the transaction amount billed to your card after the transaction has '+
					'been processed on the website.</p>' +
					'<table class="dbt-table"><tr><td rowspan="2">Debit Card (all other cards issued in india)</td><td>Less than Rs. 2000</td><td>0.75%</td></tr>'+
					'<tr><td>Greater than or Equal to &nbsp; Rs. 2000</td><td>1%</td></tr>'+
					'<tr><td>Credit Card (all cards)</td><td></td><td>1.5%</td></tr></table>'+
					'<div class="agree-lbl"><input type="checkbox" id="agree" name="agree" value="1"> <label for="agree"> I Accept</label></div><div class="agree-lbl"><input type="checkbox" id="decline" name="decline" value="1"> <label for="decline"> I Decline</label></div></div>');

			$("#background").css({"opacity" : "0.7"}).fadeIn();
			$("#large").fadeIn();			
		$("#agree").on("click", function() {
			if($(this).is(':checked')){				
				$("#makepay").attr('disabled',true);
				$.post("/payment/make/"+$("#proprty_no").val()+"/initiate", function(pres){
					if(pres.status == "OK"){
						var form = $("<form/>", {
				            action: "/payment/make/"+$("#proprty_no").val()+"/process",
				            method: "POST",
				            target: "_self"
				        },"json");
						form.append(pres.payfrm);
						$("body").append(form);
				        form.submit();
				        form.remove();
				       $("#makepay").val("Please wait...");
				       $("#background").remove();
					   $("#large").remove();
				        return false;
					}
					else if(pres.status == "E1"){
						$("#reqstatus").text("Your payable value is 0");
					}
					else if(pres.status == "E2"){
						$("#reqstatus").text("Property Number not valid");
					}		
				},"json");
			}
		});	
		$("#decline").on("click", function() {
			if($(this).is(':checked')){
				location.href="/index";
			}
		});	
	});
	
});
</script>
